@{
    ViewBag.Title = "Index";
}



<!DOCTYPE html>
<html>
<head >
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <script src="~/node_modules/angular/angular.js"></script>
    <script src="~/Scripts/app.js"></script>
    <script src="~/Scripts/MovieBrowser/MovieBrowserService.js"></script>
    <script src="~/Scripts/MovieBrowser/MovieBrowserController.js"></script>
</head>
<body ng-app="movieApp">


    <h2>Index</h2>

    <div ng-controller="MovieBrowserController">
        <input type="text" ng-model="searchTerm" ng-change="searchTerm_changed()" />
        <ul ng-repeat="movie in movies">
            <li>{{movie.Title}}</li>
        </ul>
    </div>
</html>